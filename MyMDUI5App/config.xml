<?xml version='1.0' encoding='utf-8'?>
<widget id="de.kjumybit.ui5.md_example" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Master Detail Example</name>
    <description>
        OpenUI5 Master-Detail Example.
    </description>
    <author email="kjumybit.de" href="http://kjumybit.de">
        Hagen Kr√∂ber
    </author>
    <content src="index.html" />
    <icon src="res/icon/logo_ui5.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.4" />
</widget>
