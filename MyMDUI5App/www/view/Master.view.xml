<mvc:View controllerName="kjumybit.ui5.example.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"	
	xmlns="sap.m" 
	height="100%">
	<Page id="Master" 
		title="{i18n>masterTitle}"
		backgroundDesign="Standard"
		showNavButton="false" >
		<customHeader>
			<Toolbar width="100%" >
				<Button icon="sap-icon://nav-back" press="onPressNavigationBack" />
				<ToolbarSpacer/>
			 </Toolbar>
		</customHeader>

		<content>
			<List itemPress="onNavItemPress">
				<items>
					<StandardListItem title="To Detail 1" type="Active" custom:to="detail"/>
					<StandardListItem title="To Detail 2" type="Active" custom:to="detail2"/>
				</items>
			</List>			 			
		</content>

		<footer>
		</footer>
		
	</Page>
</mvc:View>