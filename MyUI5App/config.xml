<?xml version='1.0' encoding='utf-8'?>
<widget id="de.kjumybit.demo.ws" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MyHomeDemo</name>
    <description>
        A demo app that displays events of a FHEM server using socket.io communication.
    </description>
    <author email="hagen.kroeber@kjumybit.de" href="http://kjumybit.de">
        Hagen Kr√∂ber
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
